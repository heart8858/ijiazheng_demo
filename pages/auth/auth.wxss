.logo {
  width: 200rpx;
  height: 200rpx;
  margin: auto;
  margin-top: 100rpx;
}
.logo image {
  width: 100%;
  height: 100%;
}
.logo-name {
  font-size: 32rpx;
  padding: 20rpx 0;
}
.intro {
  font-size: 30rpx;
  padding: 5rpx 0;
}

.logo-name,
.intro,
.login {
  text-align: center
}

.login {
  font-size: 28rpx;
  color: #bbb;
  margin-top: 50rpx;
  margin-bottom: 20rpx;
}

.cancel {
  width: 100rpx;
  height: 100rpx;
  margin: auto;
  margin-top: 50rpx;
  text-align: center;
  line-height: 100rpx;
}



